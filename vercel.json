{
  "version": 2,
  "builds": [
    {
      "src": "run/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "run/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "./",
    "POINTS_CSV": "data/points.csv",
    "ROUTES_CSV": "data/routes.csv",
    "OUTPUT_CSV": "data/output.csv"
  }
}
